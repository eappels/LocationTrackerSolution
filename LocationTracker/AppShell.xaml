<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LocationTracker.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:LocationTracker.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Tracking"
        ContentTemplate="{DataTemplate views:TrackingView}"
        Route="TrackingView" />
    <ShellContent Title="History"
        ContentTemplate="{DataTemplate views:HistoryView}"
        Route="HistoryView" />
</Shell>